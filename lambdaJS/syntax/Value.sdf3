module Value

imports
  
  Common
  Expression
  Lexical

context-free syntax
  
  Val.C = < <C> >
  Val.Func = < func( <ID*> ) { return <Exp> }>
  Val.Obj = < { <FieldList> } >
  
  FieldList.Empty = 
  FieldList.NonEmpty = < <Field> <FieldRest*> >
  
  Field.Prop = < <STRING> : <Val> > 
  FieldRest.PropRest = < , <STRING> : <Val> > 
