module Value

imports
  
  Common
  Expression
  Lexical

context-free syntax
  
  Val.INT = < <INT> >
  Val.BOOLEAN = < <BOOLEAN> >
  Val.UNDEFINED = < <UNDEFINED> >
  Val.NULL = < <NULL> >
  Val.STRING = < <STRING> >
  Val.Func = < func( <{ID ","}*> ) { return <Exp> }>
  Val.Obj = < { <{Field ","}*> } >
  
  Field.Prop = < <STRING> : <Val> >